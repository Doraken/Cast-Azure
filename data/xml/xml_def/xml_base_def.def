function XML_BASE_DEF_DOC_function_DEPEND
{
set -A XML_BASE_ARRAY 'Dep_obj' 'dep_func' 'func_name' 'func_libsrc'
set -A XML_fill_in_array '3' '4'
XML_BASE_Min_Array="2"
}


function XML_BASE_DEF_functionLIST_BY_FILE
{
set -A XML_BASE_ARRAY 'File Source' 'dep_func' 'func_name' 'func_libsrc'
set -A XML_fill_in_array '3' '4'
XML_BASE_Min_Array="2"

}


Package_Configuration="OK"
Sourced_OK="1"